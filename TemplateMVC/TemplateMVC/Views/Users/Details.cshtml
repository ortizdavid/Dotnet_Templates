@model TemplateMVC.Core.Models.Auth.UserData;

@{
    Layout = "_Layout";
    ViewBag.Title = "User Details";
}

<div class="card">
    <div class="card-body">
        <p><strong>User Name:</strong> @Model.UserName</p>
        <p><strong>Role:</strong> @Model.RoleName</p>
        <p><strong>Email:</strong> @Model.Email</p>
        <p><strong>Status:</strong> @(Model.IsActive ? "Active" : "Inactive")</p>
        <p><strong>Created At:</strong> @Model.CreatedAt</p>
    </div>
</div>
